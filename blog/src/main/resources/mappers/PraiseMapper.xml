<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="com.czh.blog.dao.PraiseDao">
    <select id="getPraiseByUserIdBlogId" resultType="com.czh.blog.entity.Praise">
        select * from praise where user_id = #{userId} and blog_id=#{blogId}
    </select>
</mapper>